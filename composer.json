{
  "name": "imxfly/jwt",
  "homepage": "https://github.com/imxfly/php-jwt",
  "description": "A basic library to encode and decode JSON Web Tokens (JWT) in PHP, conforming to RFC 7519.",
  "keywords": [
    "JWT",
    "JSON",
    "SSO",
    "Token"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "imxfly",
      "email": "stephenfxl@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">= 5.6.0",
    "ext-openssl": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "imxfly\\JWT\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "autoload-dev": {
    "psr-4": {
      "imxfly\\JWT\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --testdox tests"
  }
}
